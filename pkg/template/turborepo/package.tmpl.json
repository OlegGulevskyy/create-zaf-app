		"prettier": "^2.5.1",
		"turbo": "latest"
	},
	"dependencies": {
		"tailwindcss": "^3.3.2",
		"typescript": "^5.0.4"
	},
	"packageManager": "{{ .PackageManager }}@{{ .PackageManagerVersion }}",
	"name": "{{ .Name }}"
}
